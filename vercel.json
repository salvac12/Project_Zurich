{
  "version": 2,
  "name": "Project_ZUrich",
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/index.html",
      "headers": { "Cache-Control": "no-store" }
    },
    {
      "src": "/project-zurich-equity.html",
      "status": 308,
      "headers": { "Location": "/senior_financing_es.html" }
    },
    {
      "src": "/(.*)\\.html",
      "headers": { "Cache-Control": "no-store" },
      "continue": true
    }
  ]
}
