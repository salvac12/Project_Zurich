{
  "version": 2,
  "functions": {
    "api/handler.js": {
      "runtime": "@vercel/node@3.0.7",
      "includeFiles": "api/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/visitors",
      "destination": "/api/handler"
    },
    {
      "source": "/api/analytics-events",
      "destination": "/api/handler"
    }
  ]
}
